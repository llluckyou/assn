<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.assn.entity.AssnPositionEntity" table="assn_position" schema="assn">
        <id name="positionId">
            <column name="position_id" sql-type="bigint(20)"/>
        </id>
        <property name="positionName">
            <column name="position_name" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="positionDesc">
            <column name="position_desc" sql-type="varchar(50)" length="50"/>
        </property>
    </class>
</hibernate-mapping>