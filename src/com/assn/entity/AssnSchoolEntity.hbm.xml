<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.assn.entity.AssnSchoolEntity" table="assn_school" schema="assn">
        <id name="id">
            <column name="id" sql-type="bigint(20)"/>
        </id>
        <property name="schoolId">
            <column name="school_id" sql-type="varchar(10)" length="10"/>
        </property>
        <property name="schoolName">
            <column name="school_name" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="schoolType">
            <column name="school_type" sql-type="varchar(1)" length="1"/>
        </property>
        <property name="schoolLocationId">
            <column name="school_location_id" sql-type="varchar(12)" length="12"/>
        </property>
    </class>
</hibernate-mapping>