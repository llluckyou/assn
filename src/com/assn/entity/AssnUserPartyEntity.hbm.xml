<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.assn.entity.AssnUserPartyEntity" table="assn_user_party" schema="assn">
        <id name="id">
            <column name="id" sql-type="bigint(20)"/>
        </id>
        <property name="partyId">
            <column name="party_id" sql-type="bigint(20)"/>
        </property>
        <property name="userId">
            <column name="user_id" sql-type="bigint(20)"/>
        </property>
        <property name="joinFlag">
            <column name="join_flag" sql-type="varchar(1)" length="1"/>
        </property>
        <property name="joinTime">
            <column name="join_time" sql-type="datetime"/>
        </property>
        <property name="leaveTime">
            <column name="leave_time" sql-type="datetime"/>
        </property>
        <property name="positionId">
            <column name="position_id" sql-type="bigint(20)"/>
        </property>
    </class>
</hibernate-mapping>