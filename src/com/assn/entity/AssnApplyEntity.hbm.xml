<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.assn.entity.AssnApplyEntity" table="assn_apply" schema="assn">
        <id name="applyId">
            <column name="apply_id" sql-type="bigint(20)"/>
        </id>
        <property name="applyPartyId">
            <column name="apply_party_id" sql-type="bigint(20)"/>
        </property>
        <property name="applyUserId">
            <column name="apply_user_id" sql-type="bigint(20)"/>
        </property>
        <property name="applyResume">
            <column name="apply_resume" sql-type="varchar(30)" length="30"/>
        </property>
        <property name="applyDate">
            <column name="apply_date" sql-type="datetime"/>
        </property>
        <property name="applyPass">
            <column name="apply_pass" sql-type="varchar(1)" length="1"/>
        </property>
    </class>
</hibernate-mapping>